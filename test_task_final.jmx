              <stringProp name="script">import java.util.regex.Matcher;
import java.util.regex.Pattern;
import java.io.FileWriter;
import java.io.IOException;

String responseBody = prev.getResponseDataAsString(); 

Pattern idPattern = Pattern.compile(&quot;\&quot;_id\&quot;:\&quot;([^\&quot;]+)\&quot;&quot;);
Matcher idMatcher = idPattern.matcher(responseBody);


Pattern emailPattern = Pattern.compile(&quot;\&quot;email\&quot;:\&quot;([^\&quot;]+)\&quot;&quot;);
Matcher emailMatcher = emailPattern.matcher(responseBody);

if (idMatcher.find() &amp;&amp; emailMatcher.find()) {
    String _id = idMatcher.group(1); 
    String userEmail = emailMatcher.group(1); 

    String outputFile = vars.get(&quot;outputFile&quot;); 
    String outputFile2 = vars.get(&quot;outputFile2&quot;); 

    try {
        
        FileWriter writer = new FileWriter(outputFile, true);
        writer.write(_id + &quot;\n&quot;); 
        writer.close();

        
        FileWriter writer2 = new FileWriter(outputFile2, true);
        writer2.write(userEmail + &quot;\n&quot;); 
        writer2.close();
    } catch (IOException e) {
        log.error(&quot;Ошибка при записи в файл: &quot; + e.getMessage());
    }
}</stringProp>
